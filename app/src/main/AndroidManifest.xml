<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.forever.smarthair">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FaceTypeListActivity"
            android:label="@string/title_activity_face_type_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Oblonggirl" />
        <activity android:name=".Diamondgirl" />
        <activity android:name=".Heartgirl" />
        <activity android:name=".Ovalgirl" />
        <activity android:name=".Roundgirl" />
        <activity android:name=".Squaregirl"></activity>
    </application>

</manifest>